@model IEnumerable<SzelektivABC.Models.Item>

@{
    ViewBag.Title = "Összes termék";
}

<h1> Összes termék </h1>
<h3> Nincs a keresésnek megfelelő termék. Tekintse át az alábbi listát: </h3>

<ul>
    @foreach (var item in Model)
    { 
        <li> @Html.ActionLink(item.Name, "Details", new { id = item.Id } ) </li>
    }
</ul>

